{
  "manifest_version": 3,
  "name": "ASB921Finder",
  "version": "1.0",
  "description": "FÃ¼gt eine Filterfunktion im Dienstplan auf old.samariter.at hinzu",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://old.samariter.at/*"
  ],

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  },

  "content_scripts": [
    {
      "matches": ["https://old.samariter.at/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
